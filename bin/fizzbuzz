#!/bin/bash
# FizzBuzz: Print 1 to N with Fizz/Buzz/FizzBuzz substitutions

N=${1:-100}

for ((i=1; i<=N; i++)); do
    if ((i % 15 == 0)); then
        echo "FizzBuzz"
    elif ((i % 3 == 0)); then
        echo "Fizz"
    elif ((i % 5 == 0)); then
        echo "Buzz"
    else
        echo "$i"
    fi
done
